        private void machinery_item_inward_unit2BindingNavigatorSaveItem_Click(object sender, EventArgs e)
        {
            this.Validate();
            this.machinery_item_inward_unit2BindingSource.EndEdit();
            this.tableAdapterManager.UpdateAll(this.mainDBDataSet);

        }

        private void Form1_Load(object sender, EventArgs e)
        {
            // TODO: This line of code loads data into the 'mainDBDataSet.machinery_item_inward_unit2' table. You can move, or remove it, as needed.
            this.machinery_item_inward_unit2TableAdapter.Fill(this.mainDBDataSet.machinery_item_inward_unit2);

        }
